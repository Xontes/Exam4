@using Exam4.Models
@model List<MChef>
@{
    int count = 0;
}
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Id</th>
            <th scope="col">Full Name</th>
            <th scope="col">Designation</th>
            <th scope="col">Image</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            count++;
            <tr>
                <th scope="row">@count</th>
                <td>@item.Id</td>
                <td>@item.FullName</td>
                <td>@item.Designation</td>
                <td><img src="~/images/@item.ImgUrl"/></td>
                <td><a class="btn btn-dark" asp-action="Delete" asp-route-id="@item.Id">Delete</a></td>
                <td><a class="btn btn-danger" asp-action="Update" asp-route-id="@item.Id">Update</a></td>
            </tr>
            
        }
        <a asp-action="Create" class="btn btn-submit btn-primary">Create</a>
        
    </tbody>
</table>